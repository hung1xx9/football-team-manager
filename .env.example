# Firebase Configuration
# Copy this file to .env and fill in your Firebase credentials
# Get these values from Firebase Console > Project Settings > General

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# MoMo Business API Configuration
# Get these values from MoMo Developer Portal (developers.momo.vn)
VITE_MOMO_PARTNER_CODE=your_partner_code
VITE_MOMO_ACCESS_KEY=your_access_key
VITE_MOMO_SECRET_KEY=your_secret_key
VITE_MOMO_ENDPOINT=https://test-payment.momo.vn
# For production, change to: https://payment.momo.vn

# App Configuration
VITE_APP_URL=http://localhost:5173
# For production: https://your-domain.com
